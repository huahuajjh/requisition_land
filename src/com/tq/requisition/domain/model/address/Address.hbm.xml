<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tq.requisition.domain.model.address">
    <class name="Address" table="tb_address">
        <id name="id" column="id" type="org.hibernate.type.UUIDCharType">
        	<generator class="assigned"></generator>
        </id>
		<property name="pid"  column="pid" type="org.hibernate.type.UUIDCharType"/>
		<property name="title" not-null="true" column="title"/>
    </class>

</hibernate-mapping>