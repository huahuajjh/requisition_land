<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/context
						http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		<!-- 拆迁户仓储 -->
		<bean id="fmlRepository" class="com.tq.requisition.infrastructure.Repository.removeFamily.FamilyRepository">
			<constructor-arg index="0" ref="hbRepositoryContext"/>
		</bean>
		
		<!-- 拆迁户成员仓储 -->
		<bean id="fmlItemRepository" class="com.tq.requisition.infrastructure.Repository.fmlItem.FamilyItemRepository">
			<constructor-arg index="0" ref="hbRepositoryContext"/>
		</bean>
						
		<!-- 拆迁户应用层 -->
		<bean id="fmlService" class="com.tq.requisition.application.fmlMgtImpl.FamilyMgtServiceImpl">
			<constructor-arg index="0" ref="hbRepositoryContext"/>
			<constructor-arg index="1" ref="fmlRepository"/>
			<constructor-arg index="2" ref="proRepository"/>
			<constructor-arg index="3" ref="socialsecurityRepository"/>
			<constructor-arg index="4" ref="relationshipRepository"/>
			<constructor-arg index="5" ref="fmlItemRepository"/>
		</bean>
		
		<!-- 组织管理服务(应用层) -->
		<bean id="fmlItemService" class="com.tq.requisition.application.fmlMgtImpl.FamilyItemServiceImpl">
			<constructor-arg index="0" ref="hbRepositoryContext"/>
			<constructor-arg index="1" ref="fmlItemRepository"/>
			<constructor-arg index="2" ref="removedInfoRepository"/>
			<constructor-arg index="3" ref="fmlRepository"/>
		</bean>
</beans>