<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/context
						http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		<!-- 项目管理 -->
		<!-- 项目管理倉儲 -->
		<bean id="proRepository" class="com.tq.requisition.infrastructure.Repository.project.ProjectRepository">
			<constructor-arg index="0"  ref="hbRepositoryContext"/>
		</bean>
		
		<!-- 项目管理應用層業務契約 -->
		<bean id="proMgtServiceContract" class="com.tq.requisition.application.proMgt.ProMgtServiceIContractmpl">
			<constructor-arg index="0"  ref="hbRepositoryContext"/>
			<constructor-arg index="1"  ref="proRepository"/>
		</bean>
		
</beans>